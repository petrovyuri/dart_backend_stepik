{
    // Конфигурации запуска для отладки приложения
    "configurations": [
        {
            // Имя конфигурации, отображаемое в VS Code
            "name": "Launch (Stage)",
            // Тип отладчика - Dart
            "type": "dart",
            // Тип запуска - запуск новой программы
            "request": "launch",
            // Точка входа в приложение
            "program": "bin/server.dart",
            // Переменные окружения для запуска приложения
            "env": {
                "PORT": "8080",
                "IS_STAGE": "true",
                "DB_HOST": "localhost",
                "DB_NAME": "auth",
                "DB_USER": "auth",
                "DB_PASSWORD": "auth",
                "DB_PORT": "9001",
                "SALT": "secret_salt",
                "JWT_SECRET": "secret_jwt_secret",
                "JWT_ACCESS_EXP": "2",
                "JWT_REFRESH_EXP": "5",
                "DATA_REQUEST": "/users/posts/",
                "DATA_PORT": "9002",
                "DATA_HOST": "localhost",
                "INTERNAL_SECRET": "secret_internal_secret"
            }
        }
    ]
}